<Page x:Class="up0201.userPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:up0201"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="userPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="4*"/>
        </Grid.ColumnDefinitions>
        <TreeView Name="tree">
            <TreeViewItem Header="Доступные данные" Name="parrentTreeView">
                <TreeViewItem Header="Торговые центры" Selected="selectM"/>
                <TreeViewItem Header="Павильоны" Selected="selectP"/>
                
            </TreeViewItem>
        </TreeView>
        <Frame Grid.Column="1" Name="frameUser">
            
        </Frame>


    </Grid>
</Page>
